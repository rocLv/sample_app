<%= provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">

	<div class="col-sm-4 col-sm-offset-4" >
		<%= form_for(@user) do |f| %>
		  <%= render "shared/error_messages" %>

			<%= render "fields", f: f %>
			
			
			<%= f.submit "Create my account", class: "btn btn-lg btn-success" %>
			
		<% end %>
	</div>
</div>
